{
  "containerDefinitions": [
    {
      "name": "app",
      "image": "109995068952.dkr.ecr.us-east-1.amazonaws.com/shrt-backend:sha-b5edf93-production",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "LOG_CHANNEL",
          "value": "stderr"
        },
        {
          "name": "APP_ENV",
          "value": "production"
        },
        {
          "name": "APP_URL",
          "value": "http://localhost:8000"
        },
        {
          "name": "APP_DEBUG",
          "value": "false"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/shrt-backend",
          "awslogs-create-group": "true",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "shrt-backend-production",
  "taskRoleArn": "arn:aws:iam::109995068952:role/ecsTaskRole",
  "executionRoleArn": "arn:aws:iam::109995068952:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
